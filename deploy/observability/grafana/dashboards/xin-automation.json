{
  "id": null,
  "uid": "xin-automation",
  "title": "Xin Automation Health",
  "tags": [
    "xin",
    "automation"
  ],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Queue Depth by Tenant",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "automation_queue_depth",
          "legendFormat": "{{tenant_id}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Automation Latency P95",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(automation_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95 latency",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}
