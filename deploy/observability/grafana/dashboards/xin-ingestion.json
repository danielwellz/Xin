{
  "id": null,
  "uid": "xin-ingestion",
  "title": "Xin Ingestion Health",
  "tags": [
    "xin",
    "ingestion"
  ],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "In-flight Jobs by Tenant",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "ingestion_jobs_inflight",
          "legendFormat": "{{tenant_id}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Ingestion Lag Proxy (max jobs in 30m)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "max_over_time(ingestion_jobs_inflight[30m])",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}
